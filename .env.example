# ============================================
# Theta AI - Environment Variables Template
# ============================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# === Database Settings ===
DB_NAME=thetaml
DB_USER=postgres
DB_PASSWORD=your_database_password_here
DB_HOST=localhost
DB_PORT=5432

# === Email Notification Settings ===
# Used for training progress notifications
EMAIL_PASSWORD=your_email_app_password_here
EMAIL_SENDER=your_email@gmail.com
EMAIL_RECIPIENT=your_email@gmail.com
EMAIL_SMTP_SERVER=smtp.gmail.com
EMAIL_SMTP_PORT=587

# === Django Settings (for web interface) ===
DJANGO_SECRET_KEY=generate-a-secure-key-here
DJANGO_DEBUG=False
CSRF_TRUSTED_ORIGINS=https://your-domain.com

# === Theta AI Settings ===
EXTERNAL_DATA_SOURCES=true
DISCORD_DATA_ENABLED=false

# === Web Search API (optional) ===
DEBUG_WEB_SEARCH=false
WEBSEARCH_API_NAME=your_api_name
WEBSEARCH_API_KEY=your_api_key_here
